package com.funding.dto;

import java.util.Date;

public class PointVO {
	
	private int point_num;		//포인트번호
	private int mem_num;		//회원 번호
	private int point;			//포인트
	private String point_type;	//포인트유형(적립/사용)
	private Date point_regdate;	//포인트 적립/사용 일자
	
	public int getPoint_num() {
		return point_num;
	}
	public void setPoint_num(int point_num) {
		this.point_num = point_num;
	}
	public int getMem_num() {
		return mem_num;
	}
	public void setMem_num(int mem_num) {
		this.mem_num = mem_num;
	}
	public int getPoint() {
		return point;
	}
	public void setPoint(int point) {
		this.point = point;
	}
	public String getPoint_type() {
		return point_type;
	}
	public void setPoint_type(String point_type) {
		this.point_type = point_type;
	}
	public Date getPoint_regdate() {
		return point_regdate;
	}
	public void setPoint_regdate(Date point_regdate) {
		this.point_regdate = point_regdate;
	}
	
	public PointVO() {	}
	@Override
	public String toString() {
		return "PointVO [point_num=" + point_num + ", mem_num=" + mem_num + ", point=" + point + ", point_type="
				+ point_type + ", point_regdate=" + point_regdate + "]";
	}
	public PointVO(int point_num, int mem_num, int point, String point_type, Date point_regdate) {
		super();
		this.point_num = point_num;
		this.mem_num = mem_num;
		this.point = point;
		this.point_type = point_type;
		this.point_regdate = point_regdate;
	}

}
