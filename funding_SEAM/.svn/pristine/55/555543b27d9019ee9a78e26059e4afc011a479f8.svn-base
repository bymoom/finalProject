#contBox .btnBox li button {
    display: block;
    width: 300px;
    height: 280px;
    background: #b8e3ef;
    color: #000;
    border-radius: 10px;
    text-align: center;
    text-decoration: none;
    font-size: 18px;
    letter-spacing: -.5px;
    box-shadow: 3px 3px 5px 0px rgba(0,0,0,0.2);
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
}

ul {
    display: block;
    list-style: none;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 40px;
}

ul li {
	list-style: none;
	float: left;
	padding: 30px 30px;
}

