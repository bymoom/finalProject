 <section class="contact-section">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="titie-section wow fadeInDown animated  animated" style="visibility: visible;">
                            <h1 style="text-align:center;">공지사항</h1>
                            
                        </div>
                        
        <div class="card-body p-0">
          <table border="1" class="table table-striped projects" style="text-align:center;">
              <thead>
                  <tr>
                      <th style="width: 2%">
                          No
                      </th>
                     
                      <th style="width: 30%" class="text-center">
                          	제목
                      </th>
                     
                      <th style="width: 15%" class="text-center">
                         	등록일자
                      </th>
                      <th style="width: 8%">
                      		조회수
                      </th>
                      
                     
                  </tr>
              </thead>
              <tbody>
                 <c:forEach items="${noticeList }" var="ntc">
                 	<tr>
                 		<td>${ntc.notice_num }</td>
                 		<td><a style="color:black;" href="javascript:OpenWindow('<%=request.getContextPath() %>/board/notice/detail?notice_num=${ntc.notice_num }','공지사항',800,600);">${ntc.notice_title}</a></td>
                 		<td><fmt:formatDate value="${ntc.notice_regdate}" pattern="yyyy-MM-dd"/></td>
                 		<td>${ntc.notice_view_cnt}</td>
                 	</tr>
                 </c:forEach>
              </tbody>
          
          </table>
          
        </div>
                    
